little_endian_packets

custom_field custom: 1 "custom"
checksum checksum: 1 "checksum"

enum Enum : 1 {
    tag = 0,
}

group Group {
    a: 4,
    b: Enum,
    c: custom_field,
    d: checksum,
}

struct Undeclared {
    Group { e=1 },
}

struct Redeclared {
    Group { a=1, a=2 },
}

struct TypeMismatch {
    Group { a=tag, b=1, c=1, d=1 },
}

struct InvalidLiteral {
    Group { a=42 },
}

struct UndeclaredTag {
    Group { b=undeclared_tag },
}

struct Correct {
    Group { a=1, b=tag },
}
