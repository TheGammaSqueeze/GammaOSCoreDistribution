package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

rust_binary {
    name: "authfs_service",
    srcs: [
        "src/main.rs",
    ],
    edition: "2018",
    rustlibs: [
        "authfs_aidl_interface-rust",
