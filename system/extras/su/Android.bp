package {
    default_applicable_licenses: ["system_extras_su_license"],
}

license {
    name: "system_extras_su_license",
    visibility: [":__subpackages__"],
    license_kinds: [
        "SPDX-license-identifier-Apache-2.0",
    ],
    license_text: [
        "NOTICE",
    ],
}

cc_binary {
    name: "su",

    cflags: [
        "-Wall",
        "-Werror",
    ],

    srcs: ["su.cpp"],

    install_in_xbin: true,

    header_libs: ["libcutils_headers"],
}
